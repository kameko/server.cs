{
  "Logging": {
    "LogLevel": {
      "Default": "Trace",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "ServerCS": {
    "DataFolder": "./../../.data"
  },
  "Discord": {
    "TokenFilePath": "./../../.data/.token",
    "JavaScriptHostOptions": {
      "System": {
        "Directory": "./../Scripts/Commands/js",
        "StartupScriptPath": "./../Scripts/Commands/js/runtime.js",
        "Timeout": 10000,
        "LimitMemory": 4000000,
        "LimitRecursion": 30,
        "AllowDebuggerStatement": true,
        "DiscardGlobal": false,
        "AssemblyIncludes": "System",
        "Values": {
          "ServerCS Version String": {
            "VariableName": "ServerCS_Version",
            "Type": "string",
            "Value": "1.0.0.0"
          }
        }
      }
    }
  }
}
